{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
     "react-app"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/prop-types": "warn"
  }
}